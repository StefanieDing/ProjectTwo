<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<title>Calendar Widget</title>
	<link rel="stylesheet" href="calendar/calendar.css" />
</head>
<body>
	<div id="cal">
		<div class="header">
			<span class="left button" id="prev"> &lang; </span>
			<span class="left hook"></span>
			<span class="month-year" id="label"> November 2016 </span>
			<span class="right hook" id=""></span>
			<span class="right button" id="next"> &rang; </span>
		</div>
		<table id="days">
			<tr>
				<td>sun</td>
				<td>mon</td>
				<td>tue</td>
				<td>wed</td>
				<td>thu</td>
				<td>fri</td>
				<td>sat</td>
			</tr>
		</table>
		<div id="cal-frame">
			<table class="curr">
				<tr><td class="nil"></td><td class="nil"></td><td class="calDateData">1</td><td class="calDateData">2</td><td class="calDateData">3</td><td class="calDateData">4</td><td class="calDateData">5</td></tr>
				<tr><td class="calDateData">6</td><td class="calDateData">7</td><td class="calDateData">8</td><td class="calDateData">9</td><td class="calDateData">10</td><td class="today">11</td><td class="calDateData">12</td></tr>
				<tr><td class="calDateData">13</td><td class="calDateData">14</td><td class="calDateData">15</td><td class="calDateData">16</td><td class="calDateData">17</td><td class="calDateData">18</td><td class="calDateData">19</td></tr>
				<tr><td class="calDateData">20</td><td class="calDateData">21</td><td class="calDateData">22</td><td class="calDateData">23</td><td class="calDateData">24</td><td class="calDateData">25</td><td class="calDateData">26</td></tr>
				<tr><td class="calDateData">27</td><td class="calDateData">28</td><td class="calDateData">29</td><td class="calDateData">30</td><td class="nil"></td><td class="nil"></td><td class="nil"></td></tr>
			</table>
		</div>
	</div>

<!-- The Modal -->
<div id="myModal" class="modal">

<!-- Modal content -->
<div class="modal-content">
    <span class="close">x</span>
    <p>Some text in the Modal..</p>
</div>

</div>

<div class="mainHeader"></div>
<div class="mainFooter">
  <div class="socialRef">
    <div class="footer-social-icons">
    <h4 class="socialText">Follow us on</h4>
    <ul class="social-icons">
        <li><a href="" class="social-icon"> <i class="fa fa-facebook"></i></a></li>
        <li><a href="" class="social-icon"> <i class="fa fa-twitter"></i></a></li>
        <li><a href="" class="social-icon"> <i class="fa fa-linkedin"></i></a></li>
        <li><a href="" class="social-icon"> <i class="fa fa-google-plus"></i></a></li>
    </ul>
  </div>
  </div>
</div>


</body>


<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script>
<script src="calendar/calendar.js"></script>
<script>
var cal = CALENDAR();

cal.init();
</script>
</body>
</html>
